<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>	
	
	<typeAliases>
		<typeAlias type="com.kh.goodplace.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.goodplace.room.model.vo.Room" alias="Room"/>
		<typeAlias type="com.kh.goodplace.experience.model.vo.Experience" alias="Experience"/>
		<typeAlias type="com.kh.goodplace.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.goodplace.common.model.vo.Attachment" alias="Attachment"/>
		<typeAlias type="com.kh.goodplace.messages.model.vo.ChatRoom" alias="ChatRoom"/>
		<typeAlias type="com.kh.goodplace.messages.model.vo.Message" alias="Message"/>
		<typeAlias type="com.kh.goodplace.messages.model.vo.UserVO" alias="UserVO"/>
	</typeAliases>
	
	
	
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/exp-mapper.xml"/>
		<mapper resource="mappers/room-mapper.xml"/>
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/chat-mapper.xml"/>
	</mappers>
</configuration>
